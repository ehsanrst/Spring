<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="textEditor" class="com.Dotin.TextEditor">
        <property name="spellChecker">
            <bean id="spellChecker" class="com.Dotin.SpellChecker"></bean>
        </property>
    </bean>

</beans>

        <!--There is the configuration file Beans.xml which has configuration
         for the setter-based injection but using inner beans-->