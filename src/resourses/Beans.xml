<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!--To turn on Annotation wiring-->
    <context:annotation-config/>

    <bean id="profile" class="com.Dotin.Profile"></bean>

    <bean id="student1" class="com.Dotin.Student">
        <property name="name" value="Ali"/>
        <property name="age" value="21"/>
    </bean>

    <bean id="student2" class="com.Dotin.Student">
        <property name="name" value="Ehsan"/>
        <property name="age" value="27"/>
    </bean>

</beans>
